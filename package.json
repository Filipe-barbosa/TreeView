    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^0.6.15",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "storybook": "^7.5.3",
    "typescript": "*",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.1"
